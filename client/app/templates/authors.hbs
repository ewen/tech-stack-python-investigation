<h2>Authors</h2>

<ul>
  {{log model.authors.length}}
{{#each model.authors as |author|}}
  <li>{{author.name}}</li>
  <ul>
  {{#each author.books as |book|}}
    <li>{{book.title}} - {{book.displayPubDate}} ({{book.genre.name}})</li>
  {{/each}}
  </ul>
{{/each}}
</ul>

{{#link-to "index"}}Back{{/link-to}}
